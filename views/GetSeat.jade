doctype html
html
  head
    meta(charset='UTF-8')
    title AdminLTE | Dashboard
    meta(content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no', name='viewport')
    // bootstrap 3.0.2
    link(href='css/bootstrap.min.css', rel='stylesheet', type='text/css')
    // font Awesome
    link(href='css/font-awesome.min.css', rel='stylesheet', type='text/css')
    // Ionicons
    link(href='css/ionicons.min.css', rel='stylesheet', type='text/css')
    // Morris chart
    link(href='css/morris/morris.css', rel='stylesheet', type='text/css')
    // jvectormap
    link(href='css/jvectormap/jquery-jvectormap-1.2.2.css', rel='stylesheet', type='text/css')
    // fullCalendar
    link(href='css/fullcalendar/fullcalendar.css', rel='stylesheet', type='text/css')
    // Daterange picker
    link(href='css/daterangepicker/daterangepicker-bs3.css', rel='stylesheet', type='text/css')
    // bootstrap wysihtml5 - text editor
    link(href='css/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css', rel='stylesheet', type='text/css')
    // Theme style
    link(href='css/AdminLTE.css', rel='stylesheet', type='text/css')
    // HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
      script(src='https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js')
  body.skin-black
    /der logo: style can be found in header.less
    header.header
      a.logo(href='index')
        // Add the class icon to your logo image or logo icon to add the margining
        | AdminLTE
      // Header Navbar: style can be found in header.less
      nav.navbar.navbar-static-top(role='navigation')
        // Sidebar toggle button
        a.navbar-btn.sidebar-toggle(href='#', data-toggle='offcanvas', role='button')
          span.sr-only Toggle navigation
          span.icon-bar
          span.icon-bar
          span.icon-bar
        .navbar-right
          ul.nav.navbar-nav
            // Messages: style can be found in dropdown.less
            li.dropdown.messages-menu
              a.dropdown-toggle(href='#', data-toggle='dropdown')
                i.fa.fa-envelope
                span.label.label-success 4
              ul.dropdown-menu
                li.header You have 4 messages
                li
                  // inner menu: contains the actual data
                  ul.menu
                    li
                      // start message
                      a(href='#')
                        .pull-left
                          img.img-circle(src='img/avatar3.png', alt='User Image')
                        h4
                          | Support Team
                          small
                            i.fa.fa-clock-o
                            |  5 mins
                        p Why not buy a new awesome theme?
                    // end message
                    li
                      a(href='#')
                        .pull-left
                          img.img-circle(src='img/avatar2.png', alt='user image')
                        h4
                          | AdminLTE Design Team
                          small
                            i.fa.fa-clock-o
                            |  2 hours
                        p Why not buy a new awesome theme?
                    li
                      a(href='#')
                        .pull-left
                          img.img-circle(src='img/avatar.png', alt='user image')
                        h4
                          | Developers
                          small
                            i.fa.fa-clock-o
                            |  Today
                        p Why not buy a new awesome theme?
                    li
                      a(href='#')
                        .pull-left
                          img.img-circle(src='img/avatar2.png', alt='user image')
                        h4
                          | Sales Department
                          small
                            i.fa.fa-clock-o
                            |  Yesterday
                        p Why not buy a new awesome theme?
                    li
                      a(href='#')
                        .pull-left
                          img.img-circle(src='img/avatar.png', alt='user image')
                        h4
                          | Reviewers
                          small
                            i.fa.fa-clock-o
                            |  2 days
                        p Why not buy a new awesome theme?
                li.footer
                  a(href='#') See All Messages
            // Notifications: style can be found in dropdown.less
            li.dropdown.notifications-menu
              a.dropdown-toggle(href='#', data-toggle='dropdown')
                i.fa.fa-warning
                span.label.label-warning 10
              ul.dropdown-menu
                li.header You have 10 notifications
                li
                  // inner menu: contains the actual data
                  ul.menu
                    li
                      a(href='#')
                        i.ion.ion-ios7-people.info
                        |  5 new members joined today
                    li
                      a(href='#')
                        i.fa.fa-warning.danger
                        |  Very long description here that may not fit into the page and may cause design problems
                    li
                      a(href='#')
                        i.fa.fa-users.warning
                        |  5 new members joined
                    li
                      a(href='#')
                        i.ion.ion-ios7-cart.success
                        |  25 sales made
                    li
                      a(href='#')
                        i.ion.ion-ios7-person.danger
                        |  You changed your username
                li.footer
                  a(href='#') View all
            // Tasks: style can be found in dropdown.less
            li.dropdown.tasks-menu
              a.dropdown-toggle(href='#', data-toggle='dropdown')
                i.fa.fa-tasks
                span.label.label-danger 9
              ul.dropdown-menu
                li.header You have 9 tasks
                li
                  // inner menu: contains the actual data
                  ul.menu
                    li
                      // Task item
                      a(href='#')
                        h3
                          | Design some buttons
                          small.pull-right 20%
                        .progress.xs
                          .progress-bar.progress-bar-aqua(style='width: 20%', role='progressbar', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
                            span.sr-only 20% Complete
                    // end task item
                    li
                      // Task item
                      a(href='#')
                        h3
                          | Create a nice theme
                          small.pull-right 40%
                        .progress.xs
                          .progress-bar.progress-bar-green(style='width: 40%', role='progressbar', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
                            span.sr-only 40% Complete
                    // end task item
                    li
                      // Task item
                      a(href='#')
                        h3
                          | Some task I need to do
                          small.pull-right 60%
                        .progress.xs
                          .progress-bar.progress-bar-red(style='width: 60%', role='progressbar', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
                            span.sr-only 60% Complete
                    // end task item
                    li
                      // Task item
                      a(href='#')
                        h3
                          | Make beautiful transitions
                          small.pull-right 80%
                        .progress.xs
                          .progress-bar.progress-bar-yellow(style='width: 80%', role='progressbar', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
                            span.sr-only 80% Complete
                    // end task item
                li.footer
                  a(href='#') View all tasks
            // User Account: style can be found in dropdown.less
            li.dropdown.user.user-menu
              a.dropdown-toggle(href='#', data-toggle='dropdown')
                i.glyphicon.glyphicon-user
                span
                  | #{user.username} 
                  i.caret
              ul.dropdown-menu
                // User image
                li.user-header.bg-light-blue
                  img.img-circle(src='img/avatar3.png', alt='User Image')
                  p
                    | #{user.username} - Web Developer
                    small Member since Nov. 2012
                // Menu Body
                li.user-body
                  .col-xs-4.text-center
                    a(href='#') Followers
                  .col-xs-4.text-center
                    a(href='#') Sales
                  .col-xs-4.text-center
                    a(href='#') Friends
                // Menu Footer
                li.user-footer
                  .pull-left
                    a.btn.btn-default.btn-flat(href='#') Profile
                  .pull-right
                    a.btn.btn-default.btn-flat(href='/logout') Sign out
    .wrapper.row-offcanvas.row-offcanvas-left
      // Left side column. contains the logo and sidebar
      aside.left-side.sidebar-offcanvas
        // sidebar: style can be found in sidebar.less
        section.sidebar
          // Sidebar user panel
          .user-panel
            .pull-left.image
              img.img-circle(src='img/avatar3.png', alt='User Image')
            .pull-left.info
              p Hello, #{user.username}
              a(href='#')
                i.fa.fa-circle.text-success
                |  Online
          // search form
          form.sidebar-form(action='#', method='get')
            .input-group
              input.form-control(type='text', name='q', placeholder='Search...')
              span.input-group-btn
                button#search-btn.btn.btn-flat(type='submit', name='seach')
                  i.fa.fa-search
          // /.search form
          // sidebar menu: : style can be found in sidebar.less
          ul.sidebar-menu
            li.active
              a(href='GetSeat')
                i.fa.fa-dashboard
                span Get Seat
            li
              a(href='pages/widgets.html')
                i.fa.fa-th
                span Lend Seat
                small.badge.pull-right.bg-green new
            li.treeview
              a(href='#')
                i.fa.fa-bar-chart-o
                span Assign&reserve State
                i.fa.fa-angle-left.pull-right
              ul.treeview-menu
                li
                  a(href='pages/charts/morris.html')
                    i.fa.fa-angle-double-right
                    |  Morris
                li
                  a(href='pages/charts/flot.html')
                    i.fa.fa-angle-double-right
                    |  Flot
                li
                  a(href='pages/charts/inline.html')
                    i.fa.fa-angle-double-right
                    |  Inline charts
            li.treeview
              a(href='#')
                i.fa.fa-edit
                span Reserve Facility
                i.fa.fa-angle-left.pull-right
              ul.treeview-menu
                li
                  a(href='pages/forms/general.html')
                    i.fa.fa-angle-double-right
                    |  General Elements
                li
                  a(href='pages/forms/advanced.html')
                    i.fa.fa-angle-double-right
                    |  Advanced Elements
                li
                  a(href='pages/forms/editors.html')
                    i.fa.fa-angle-double-right
                    |  Editors
            li
              a(href='pages/mailbox.html')
                i.fa.fa-envelope
                span Send complaint
                small.badge.pull-right.bg-yellow 12
        // /.sidebar
      // Right side column. Contains the navbar and content of the page
      aside.right-side
        // Content Header (Page header)
        section.content-header
          h1
            | GetSeat page
            small Control panel
          ol.breadcrumb
            li
              a(href='#')
                i.fa.fa-dashboard
                |  Home
            li.active Get Seat
          h2 Our Services
          hr.small
          .row
            .col-md-3.col-sm-6
              .service-item(style='text-align:center;')
                span.fa-stack.fa-4x
                  i.fa.fa-circle.fa-stack-2x
                  i.fa.fa-book.fa-stack-1x.text-primary
                h4
                  strong 1 Floor
                a.btn.btn-light(href='/map', data-toggle='modal') Learn More
            .col-md-3.col-sm-6
              .service-item(style='text-align:center;')
                span.fa-stack.fa-4x
                  i.fa.fa-circle.fa-stack-2x
                  i.fa.fa-book.fa-stack-1x.text-primary
                h4
                  strong 2 Floor
                a.btn.btn-light(href='/map', data-toggle='modal') Learn More
            .col-md-3.col-sm-6
              .service-item(style='text-align:center;')
                span.fa-stack.fa-4x
                  i.fa.fa-circle.fa-stack-2x
                  i.fa.fa-book.fa-stack-1x.text-primary
                h4
                  strong 3 Floor
                a.btn.btn-light(href='/map', data-toggle='modal') Learn More
            .col-md-3.col-sm-6
              .service-item(style='text-align:center;')
                span.fa-stack.fa-4x
                  i.fa.fa-circle.fa-stack-2x
                  i.fa.fa-book.fa-stack-1x.text-primary
                h4
                  strong 4 Floor
                a.btn.btn-light(href='/map', data-toggle='modal') Learn More
        // Main content
        section.content
        // /.content
      // /.right-side
